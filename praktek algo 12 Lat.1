class mahasiswi:
    
   def __init__(self,nama, nim, angkatan):
        
        self.nama = nama
        self.nim = nim
        self.angkatan = angkatan
        
   def biodata_mahasiswi(self):
       print("\nnama kamu adalah : ",self.nama)
       print("nim kamu : ",self.nim)
       print("kamu angkatan ke: ",self.angkatan)
       
nama = str(input("masukkan nama kamu : "))
nim = str(input("masukkan nim kamu : "))
angkatan = str(input("masukkan angkatan kamu: "))

biodata = mahasiswi(nama, nim , angkatan)
