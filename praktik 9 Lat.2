x = int(input("masukkan angkanya: "))
y = int(input("masukkan pangkatnya: "))

def pangkat(x,y):
    if y>=1:
        return x* pangkat(x, y-1)
    return x

hasil = pangkat(x,y)
print(f"hasilnya adalah: {hasil} ")
